    this.init();\r\n    this.getyonghuCount();\r\n    this.yonghuChat1();\r\n    this.getshiwuzhaolingCount();\r\n    this.shiwuzhaolingChat1();\r\n    this.getxunwuqishiCount();\r\n    this.xunwuqishiChat1();\r\n  },\r\n  methods:{\r\n    init(){\r\n        if(this.$storage.get('Token')){\r\n        this.$http({\r\n            url: `${this.$storage.get('sessionTable')}/session`,\r\n            method: \"get\"\r\n        }).then(({ data }) => {\r\n            if (data && data.code != 0) {\r\n            router.push({ name: 'login' })\r\n            }\r\n        });\r\n        }else{\r\n            router.push({ name: 'login' })\r\n        }\r\n    },\r\n    getyonghuCount() {\r\n        this.$http({\r\n            url: `yonghu/count`,\r\n            method: \"get\"\r\n        }).then(({\r\n            data\r\n        }) => {\r\n            if (data && data.code == 0) {\r\n                this.yonghuCount = data.data\r\n            }\r\n        })\r\n    },\r\n\r\n    yonghuChat1() {\r\n      this.$nextTick(()=>{\r\n\r\n        var yonghuChart1 = echarts.init(document.getElementById(\"yonghuChart1\"),'macarons');\r\n        this.$http({\r\n            url: \"yonghu/group/xingbie\",\r\n            method: \"get\",\r\n        }).then(({ data }) => {\r\n            if (data && data.code === 0) {\r\n                let res = data.data;\r\n                let xAxis = [];\r\n                let yAxis = [];\r\n                let pArray = []\r\n                for(let i=0;i<res.length;i++){\r\n                    xAxis.push(res[i].xingbie);\r\n                    yAxis.push(parseFloat((res[i].total)));\r\n                    pArray.push({\r\n                        value: parseFloat((res[i].total)),\r\n                        name: res[i].xingbie\r\n                    })\r\n                }\r\n                var option = {};\r\n                option = {\r\n                        title: {\r\n                            text: '男女比例',\r\n                            left: 'center'\r\n                        },\r\n                        tooltip: {\r\n                          trigger: 'item',\r\n                          formatter: '{b} : {c} ({d}%)'\r\n                        },\r\n                        series: [\r\n                            {\r\n                                type: 'pie',\r\n                                radius: ['25%', '55%'],\r\n                                center: ['50%', '60%'],\r\n                                data: pArray,\r\n                                emphasis: {\r\n                                    itemStyle: {\r\n                                        shadowBlur: 10,\r\n                                        shadowOffsetX: 0,\r\n                                        shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                                    }\r\n                                }\r\n                            }\r\n                        ]\r\n                };\r\n                // 使用刚指定的配置项和数据显示图表。\r\n                yonghuChart1.setOption(option);\r\n                  //根据窗口的大小变动图表\r\n                window.onresize = function() {\r\n                    yonghuChart1.resize();\r\n                };\r\n            }\r\n        });\r\n      })\r\n    },\r\n\r\n\r\n\r\n\r\n\r\n\r\n    getshiwuzhaolingCount() {\r\n        this.$http({\r\n            url: `shiwuzhaoling/count`,\r\n            method: \"get\"\r\n        }).then(({\r\n            data\r\n        }) => {\r\n            if (data && data.code == 0) {\r\n                this.shiwuzhaolingCount = data.data\r\n            }\r\n        })\r\n    },\r\n\r\n    shiwuzhaolingChat1() {\r\n      this.$nextTick(()=>{\r\n\r\n        var shiwuzhaolingChart1 = echarts.init(document.getElementById(\"shiwuzhaolingChart1\"),'macarons');\r\n        this.$http({\r\n            url: \"shiwuzhaoling/group/faburiqi\",\r\n            method: \"get\",\r\n        }).then(({ data }) => {\r\n            if (data && data.code === 0) {\r\n                let res = data.data;\r\n                let xAxis = [];\r\n                let yAxis = [];\r\n                let pArray = []\r\n                for(let i=0;i<res.length;i++){\r\n                    xAxis.push(res[i].faburiqi);\r\n                    yAxis.push(parseFloat((res[i].total)));\r\n                    pArray.push({\r\n                        value: parseFloat((res[i].total)),\r\n                        name: res[i].faburiqi\r\n                    })\r\n                }\r\n                var option = {};\r\n                option = {\r\n                    title: {\r\n                        text: '日失物量',\r\n                        left: 'center'\r\n                    },\r\n                    tooltip: {\r\n                      trigger: 'item',\r\n                      formatter: '{b} : {c}'\r\n                    },\r\n                    xAxis: {\r\n                        type: 'category',\r\n                        data: xAxis\r\n                    },\r\n                    yAxis: {\r\n                        type: 'value'\r\n                    },\r\n                    series: [{\r\n                        data: yAxis,\r\n                        type: 'bar'\r\n                    }]\r\n                };\r\n                // 使用刚指定的配置项和数据显示图表。\r\n                shiwuzhaolingChart1.setOption(option);\r\n                  //根据窗口的大小变动图表\r\n                window.onresize = function() {\r\n                    shiwuzhaolingChart1.resize();\r\n                };\r\n            }\r\n        });\r\n      })\r\n    },\r\n\r\n\r\n\r\n\r\n\r\n\r\n    getxunwuqishiCount() {\r\n        this.$http({\r\n            url: `xunwuqishi/count`,\r\n            method: \"get\"\r\n        }).then(({\r\n            data\r\n        }) => {\r\n            if (data && data.code == 0) {\r\n                this.xunwuqishiCount = data.data\r\n            }\r\n        })\r\n    },\r\n\r\n    xunwuqishiChat1() {\r\n      this.$nextTick(()=>{\r\n\r\n        var xunwuqishiChart1 = echarts.init(document.getElementById(\"xunwuqishiChart1\"),'macarons');\r\n        this.$http({\r\n            url: \"xunwuqishi/group/faburiqi\",\r\n            method: \"get\",\r\n        }).then(({ data }) => {\r\n            if (data && data.code === 0) {\r\n                let res = data.data;\r\n                let xAxis = [];\r\n                let yAxis = [];\r\n                let pArray = []\r\n                for(let i=0;i<res.length;i++){\r\n                    xAxis.push(res[i].faburiqi);\r\n                    yAxis.push(parseFloat((res[i].total)));\r\n                    pArray.push({\r\n                        value: parseFloat((res[i].total)),\r\n                        name: res[i].faburiqi\r\n                    })\r\n                }\r\n                var option = {};\r\n                option = {\r\n                    title: {\r\n                        text: '日寻物量',\r\n                        left: 'center'\r\n                    },\r\n                    tooltip: {\r\n                      trigger: 'item',\r\n                      formatter: '{b} : {c}'\r\n                    },\r\n                    xAxis: {\r\n                        type: 'category',\r\n                        boundaryGap: false,\r\n                        data: xAxis\r\n                    },\r\n                    yAxis: {\r\n                        type: 'value'\r\n                    },\r\n                    series: [{\r\n                        data: yAxis,\r\n                        type: 'line',\r\n                        areaStyle: {}\r\n                    }]\r\n                };\r\n                // 使用刚指定的配置项和数据显示图表。\r\n                xunwuqishiChart1.setOption(option);\r\n                  //根据窗口的大小变动图表\r\n                window.onresize = function() {\r\n                    xunwuqishiChart1.resize();\r\n                };\r\n            }\r\n        });\r\n      })\r\n    },\r\n\r\n\r\n\r\n\r\n\r\n\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" sc