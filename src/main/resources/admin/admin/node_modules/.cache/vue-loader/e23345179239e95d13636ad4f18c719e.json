":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px\",\"background\":\"rgba(162, 153, 136, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('yonghu','新增')\" type=\"success\" @click=\"addOrUpdateHandler()\">新增</el-button>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px\",\"background\":\"rgba(162, 153, 136, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('yonghu','删除')\" :disabled=\"dataListSelections.length <= 0\" type=\"danger\" @click=\"deleteHandler()\">删除</el-button>\n\n\n\n\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px\",\"background\":\"rgba(162, 153, 136, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('yonghu','男女比例')\" type=\"warning\" @click=\"chartDialog1()\">男女比例</el-button>\n\t\t\t\t</el-row>\n\t\t\t</el-form>\r\n\t\t\t\n\t\t\t<!-- <div> -->\n\t\t\t\t<el-table class=\"tables\"\r\n\t\t\t\t\t:stripe='false'\r\n\t\t\t\t\t:style='{\"padding\":\"0\",\"borderColor\":\"#666\",\"margin\":\"0 0 0 200px \",\"borderWidth\":\"1px\",\"background\":\"#fff\",\"width\":\"89%\",\"borderStyle\":\"solid\"}' \r\n\t\t\t\t\tv-if=\"isAuth('yonghu','查看')\"\n\t\t\t\t\t:data=\"dataList\"\n\t\t\t\t\tv-loading=\"dataListLoading\"\n\t\t\t\t@selection-change=\"selectionChangeHandler\">\n\t\t\t\t\t<el-table-column :resizable='true' type=\"selection\" align=\"center\" width=\"50\"></el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' label=\"索引\" type=\"index\" width=\"50\" />\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"zhanghao\"\n\t\t\t\t\tlabel=\"账号\">\n