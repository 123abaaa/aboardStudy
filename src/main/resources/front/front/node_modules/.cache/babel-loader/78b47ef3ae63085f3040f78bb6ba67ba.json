\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                  v-model=\"ruleForm.shideshijian\" \r\n                  type=\"datetime\"\r\n                  placeholder=\"拾得时间\">\r\n              </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"linear-gradient(180deg, rgba(254,215,217,1) 0%, rgba(254,215,217,1) 51%, rgba(255,182,193,1) 51%, rgba(255,182,193,1) 100%)\"}' label=\"认领地址\" prop=\"renlingdizhi\">\r\n            <el-input v-model=\"ruleForm.renlingdizhi\" \r\n                placeholder=\"认领地址\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"linear-gradient(180deg, rgba(254,215,217,1) 0%, rgba(254,215,217,1) 51%, rgba(255,182,193,1) 51%, rgba(255,182,193,1) 100%)\"}'  label=\"状态\" prop=\"zhuangtai\">\r\n            <el-select v-model=\"ruleForm.zhuangtai\" placeholder=\"请选择状态\" >\r\n              <el-option\r\n                  v-for=\"(item,index) in zhuangtaiOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"linear-gradient(180deg, rgba(254,215,217,1) 0%, rgba(254,215,217,1) 51%, rgba(255,182,193,1) 51%, rgba(255,182,193,1) 100%)\"}' label=\"发布日期\" prop=\"faburiqi\" >\r\n              <el-date-picker\r\n                  format=\"yyyy 年 MM 月 dd 日\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                 